{
  "name": "form-gather",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "size-limit": [
    {
      "path": "dist/*.js",
      "limit": "1 KB"
    }
  ],
  "browserslist": [
    "chrome >= 6",
    "firefox >= 21",
    "ie >= 11"
  ],
  "scripts": {
    "start": "jest --watch",
    "build": "tsc",
    "prepublish": "tsc --outDir .",
    "test": "npm run build && npm run test:jest && npm run test:compat && npm run test:size",
    "test:jest": "jest",
    "test:karma": "karma start karma.conf.js",
    "test:size": "size-limit",
    "test:compat": "eslint ./dist",
    "lint": "tslint --project .",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {},
  "devDependencies": {
    "@size-limit/preset-small-lib": "^2.2.3",
    "@types/jest": "^24.0.23",
    "eslint": "^6.8.0",
    "eslint-plugin-compat": "^3.3.0",
    "jasmine-core": "^3.5.0",
    "jest": "^24.9.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-typescript": "^4.1.1",
    "puppeteer": "^2.0.0",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "3.7.3"
  }
}
